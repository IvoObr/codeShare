{
  "compilerOptions": {
  "target": "es6",
  "module": "commonjs",
  "outDir": "dist",
  "strict": true,
  "baseUrl": "./",
  "noEmit": false,
  "inlineSourceMap": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  },
  "include": ["**/src/**/*.ts","**/_tests_/**/*.ts", "rmlogs.js", "chat-server/public/*.js"],
  "exclude": ["**/src/public/", "**/dist/"]
}